@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

<div class="text-center">
    CHANGE2
  <table>
 <thead>
     <tr>
        <th>ProductID</th>
        <th>ProductName</th>
        <th>Quantity</th>
     </tr>
 </thead>
 <tbody>
     @foreach (var product in Model.Products)
            {
                <tr>
                    <td>@product.ProductID</td>
                    <td>@product.ProductName</td>
                    <td>@product.Quantity</td>
                </tr>
            }
</tbody>
  </table>
</div>


